{
  "name": "@onterapi/backend",
  "version": "0.16.1",
  "description": "OnTerapi v4 - Sistema de Gestao para Terapias Integrativas",
  "private": true,
  "license": "PROPRIETARY",
  "author": {
    "name": "OnTerapi Team",
    "email": "dev@onterapi.com.br"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "dev": "nest start --watch",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "vercel-build": "npm run build",
    "test": "npm run test:unit",
    "test:watch": "jest --watch",
    "test:cov": "jest --config jest.config.js --coverage --runInBand",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "check:dry": "npm run lint -- --max-warnings 0",
    "check:quality": "npm run lint -- --max-warnings 0 && npm run test:cov",
    "migration:create": "typeorm-ts-node-commonjs migration:create",
    "migration:generate": "typeorm-ts-node-commonjs migration:generate",
    "migration:run": "typeorm-ts-node-commonjs migration:run",
    "migration:revert": "typeorm-ts-node-commonjs migration:revert",
    "backfill:user-slugs": "ts-node scripts/backfill-supabase-user-slugs.ts",
    "sync:users": "ts-node scripts/sync-users-from-supabase.ts",
    "seed:run": "ts-node src/infrastructure/database/seeds/run-seed.ts",
    "docs:generate": "compodoc -p tsconfig.json -s",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "prune:users": "ts-node scripts/prune-users.ts",
    "assign:super-admin-tenant": "ts-node scripts/assign-super-admin-tenant.ts",
    "test:unit": "jest --config jest.config.js --runInBand --testPathPattern=test/unit",
    "test:int": "jest --config jest.config.js --runInBand --testPathPattern=test/integration"
  },
  "dependencies": {
    "@nestjs/bull": "^10.0.1",
    "@nestjs/common": "^10.3.0",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.3.0",
    "@nestjs/event-emitter": "^2.0.3",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.3.0",
    "@nestjs/swagger": "^7.1.17",
    "@nestjs/terminus": "^10.3.0",
    "@nestjs/typeorm": "^10.0.1",
    "@supabase/supabase-js": "^2.56.1",
    "@types/qrcode": "^1.5.5",
    "@types/speakeasy": "^2.0.10",
    "bcryptjs": "^2.4.3",
    "bull": "^4.11.5",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "express": "^5.1.0",
    "helmet": "^7.1.0",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pg": "^8.16.3",
    "qrcode": "^1.5.4",
    "reflect-metadata": "^0.2.1",
    "rxjs": "^7.8.1",
    "speakeasy": "^2.0.0",
    "typeorm": "^0.3.19",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "zod": "^3.22.4",
    "zod-to-json-schema": "^3.22.4",
    "resend": "^3.4.0"
  },
  "devDependencies": {
    "@compodoc/compodoc": "^1.1.23",
    "@nestjs/cli": "^10.2.1",
    "@nestjs/schematics": "^10.0.3",
    "@nestjs/testing": "^10.3.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/bull": "^4.10.0",
    "@types/express": "^4.17.23",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.6",
    "@types/passport-jwt": "^4.0.0",
    "@types/passport-local": "^1.0.38",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "conventional-changelog-cli": "^4.1.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.2",
    "jest": "^29.7.0",
    "prettier": "^3.1.1",
    "rimraf": "^5.0.5",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.16",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3"
  }
}
